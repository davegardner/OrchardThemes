@model dynamic

@{
    Html.TitleForPage(T("Change Password"));
}

<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <div class="alert alert-dismissable alert-success">
                <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                <i class="fa fa-check-circle"></i>&nbsp;&nbsp;@T("Your password has been changed successfully.")
            </div>
        </div>
    </div>
</div>
